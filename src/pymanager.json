    "additional_config": "%PYTHON_MANAGER_CONFIG%",

    "pep514_root": "HKEY_CURRENT_USER\\Software\\Python",
    "start_folder": "Python",

    "launcher_exe": "./templates/launcher.exe",
    "launcherw_exe": "./templates/launcherw.exe"
}