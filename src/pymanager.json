{
    "install": {
        "source": "./bundled/index.json"
    },
    "registry_override_key": "HKEY_LOCAL_MACHINE\\Software\\Policies\\Python\\PyManager",

    "bundled_dir": "./bundled",
    "logs_dir": "%PYTHON_MANAGER_LOGS%",

    "default_tag": "3",
